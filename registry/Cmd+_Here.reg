Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\Directory\Background\shell\Cmd+ Here]
"Icon"="cmd.exe"

[HKEY_CLASSES_ROOT\Directory\Background\shell\Cmd+ Here\command]
@="cmd.exe /k C:\\Console\\profile.cmd \"%v\""

[HKEY_CLASSES_ROOT\Directory\Background\shell\Cmd+ Here (Admin)]
"Icon"="cmd.exe"

[HKEY_CLASSES_ROOT\Directory\Background\shell\Cmd+ Here (Admin)\command]
@="\"C:\\Console\\utils\\nircmd.exe\" elevate cmd.exe /k C:\\Console\\profile.cmd \"%v\""

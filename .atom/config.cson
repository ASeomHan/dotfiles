"*":
  core:
    disabledPackages: [
      "exception-reporting"
      "metrics"
    ]
    themes: [
      "one-dark-ui"
      "one-dark-vivid-syntax"
    ]
  editor:
    fontFamily: "Fira Mono, D2Coding"
    fontSize: 15
    lineHeight: 1.4
    scrollPastEnd: true
  "file-icons":
    coloured: false
  welcome:
    showOnStartup: false
